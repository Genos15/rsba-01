type Group {
    id: ID!
    name: String!
    description: String
    createdAt: String
    editedAt: String
    parent: Group
    priority: Int
    isAnalytic: Boolean
    isStaging: Boolean
}

type GroupEdge {
    cursor: String
    node: Group
}

type GroupConnection {
    edges: [GroupEdge]
    pageInfo: PageInfo
}
