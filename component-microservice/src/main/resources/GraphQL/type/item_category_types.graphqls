type ItemCategory {
    id: ID!
    name: String!
    description: String
    createdAt: DateTime
    editedAt: DateTime
    children: [ItemCategory!]
    subItems: [Item!]
}

type ItemCategoryEdge {
    cursor: String
    node: ItemCategory
}

type ItemCategoryConnection {
    edges: [ItemCategoryEdge]
    pageInfo: PageInfo
}
